 }

 @ARTICLE{Hata2016-sg,
   title    = "Body-mounted robotic instrument guide for image-guided
               cryotherapy of renal cancer",
   author   = "Hata, Nobuhiko and Song, Sang-Eun and Olubiyi, Olutayo and
