 @ARTICLE{Arata2010-ze,
   title    = "Open core control software for surgical robots",
   author   = "Arata, Jumpei and Kozuka, Hiroaki and Kim, Hyung Wook and
               Takesue, Naoyuki and Vladimirov, B and Sakaguchi, Masamichi and
