 }

 @ARTICLE{Song2013-za,
   title    = "Development and preliminary evaluation of a motorized needle
               guide template for {MRI-guided} targeted prostate biopsy",
   author   = "Song, Sang-Eun and Tokuda, Junichi and Tuncali, Kemal and
